fun Race(p1c: colour, p2c: colour, scoremax: int) -> int {
    let p1score: int = 0;
    let p2score: int = 0;
    while ((p1score < scoremax) and (p2score < scoremax)) {
        let p1toss: int = randi 1000;
        let p2toss: int = randi 1000;
        if (p1toss > p2toss) {
            p1score = p1score + 1;
            write 1, p1score, p1c;
        } else {
            p2score = p2score + 1;
            write 2, p2score, p2c;
        }
        delay 100;
    }
    if (p2score > p1score) {
        return 2;
    }
    return 1;
}

